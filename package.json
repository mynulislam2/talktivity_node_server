        "axios": "^1.10.0",
        "bcrypt": "^5.1.1",
        "cors": "^2.8.5",
        "dotenv": "^16.5.0",
        "emoji-mart": "^5.6.0",
        "express": "^4.21.2",
        "express-rate-limit": "^8.0.1",
        "express-validator": "^7.2.1",
        "google-auth-library": "^10.1.0",
        "helmet": "^8.1.0",
