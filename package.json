{
    "name": "Talktivity Server",
    "version": "1.0.0",
    "description": "Talktivity Server",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon server.js",
        "migrate": "node migrate-existing-users.js",
